{
  "name": "b-cube-ico",
  "version": "1.0.0",
  "description": "Token and staking contracts",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "@chainlink/contracts": "0.0.11",
    "@openzeppelin/contracts": "2.5.1",
    "big-number": "2.0.0",
    "bignumber.js": "9.0.1",
    "ethers": "^5.0.31",
    "ganache-time-traveler": "1.0.15",
    "web3": "1.3.0"
  },
  "devDependencies": {
    "@truffle/hdwallet-provider": "^1.2.2",
    "chai": "4.2.0",
    "eth-gas-reporter": "0.2.19",
    "mocha": "8.2.1",
    "solhint": "^3.3.4",
    "solidity-coverage": "0.7.11",
    "truffle-assertions": "0.9.2"
  },
  "scripts": {
    "clean": "rm -rf .config .npm .node-gyp node_modules && npm install",
    "cleanSol": "rm -rf build/contracts",
    "compile": "clear && truffle compile && npm run lint",
    "coverage": "truffle run coverage",
    "lint": "solhint \"contracts/**/*.sol\"",
    "migrate": "clear && truffle migrate",
    "migrateAll": "clear && truffle migrate --reset",
    "recompile": "clear && npm run cleanSol && truffle compile --all && npm run lint",
    "test": "clear && truffle test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erwan-rouzel/b-cube-ico.git"
  },
  "author": "b-cube.ai",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/erwan-rouzel/b-cube-ico/issues"
  },
  "homepage": "https://github.com/erwan-rouzel/b-cube-ico#readme"
}
